{
  "name": "redash-monorepo",
  "version": "1.0.0",
  "description": "The frontend part of Redash.",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/*",
    "client/cypress"
  ],
  "scripts": {
    "start": "yarn build && yarn start:app",
    "start:app": "yarn workspace @redash/app start",
    "dev": "npm-run-all --parallel watch:viz watch:app",
    "bundle": "bin/bundle-extensions",
    "clean": "yarn clean:app && yarn clean:viz",
    "clean:app": "yarn workspace @redash/app clean",
    "clean:viz": "yarn workspace @redash/viz clean",
    "build": "yarn build:viz && yarn build:app",
    "build:app": "yarn clean:app && yarn workspace @redash/app build",
    "build:viz": "yarn clean:viz && yarn workspace @redash/viz build:babel",
    "watch:app": "yarn workspace @redash/app watch",
    "watch:viz": "yarn workspace @redash/viz watch:babel",
    "watch": "npm-run-all --parallel watch:*",
    "webpack-dev-server": "webpack-dev-server",
    "lint": "yarn workspace @redash/app lint",
    "lint:fix": "yarn workspace @redash/app lint:fix",
    "lint:ci": "yarn workspace @redash/app lint:ci",
    "prettier:check:app": "yarn workspace @redash/app prettier:check",
    "prettier:check:viz": "yarn workspace @redash/viz prettier:check",
    "prettier:check": "yarn prettier:check:app && yarn prettier:check:viz",
    "prettier:write:app": "yarn workspace @redash/app prettier:write",
    "prettier:write:viz": "yarn workspace @redash/viz prettier:write",
    "prettier:write": "yarn prettier:write:app && yarn prettier:write:viz",
    "type-check:app": "yarn workspace @redash/app type-check",
    "type-check:viz": "yarn workspace @redash/viz type-check",
    "type-check": "yarn type-check:app && yarn type-check:viz",
    "check:all": "yarn type-check && yarn lint && yarn prettier:check",
    "jest": "TZ=Africa/Khartoum jest",
    "test:app": "yarn workspace @redash/app test",
    "test:viz": "yarn workspace @redash/viz test",
    "test": "yarn test:app && yarn test:viz",
    "cy": "yarn workspace @redash/e2e cy",
    "cy:open-ci": "yarn workspace @redash/e2e cy:open-ci",
    "cy:run-ci": "yarn workspace @redash/e2e cy:run-ci",
    "cy:ui": "yarn workspace @redash/e2e cy:ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getredash/redash.git"
  },
  "engines": {
    "node": "^14.16.1",
    "yarn": "^1.22.10"
  },
  "author": "Redash Contributors",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/getredash/redash/issues"
  },
  "homepage": "https://redash.io/",
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/lodash": "^4.14.157",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "core-js": "^2.4.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "typescript": "^4.1.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "nyc": {
    "include": [
      "packages/**"
    ]
  }
}
