{
  "name": "@redash/app",
  "version": "9.0.0-beta",
  "description": "Redash Web App UI",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rm -rf dist",
    "build": "yarn clean && NODE_ENV=production webpack",
    "watch": "webpack --watch --progress --colors -d",
    "lint:base": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore --ext .js --ext .jsx --ext .ts --ext .tsx ./src",
    "lint": "yarn lint:base",
    "lint:fix": "yarn lint:base --fix",
    "lint:ci": "yarn lint --max-warnings 0 --format junit --output-file /tmp/test-results/eslint/results.xml",
    "prettier:check": "prettier --check 'src/app/**/*.{js,jsx,ts,tsx}'",
    "prettier:write": "prettier --write 'src/app/**/*.{js,jsx,ts,tsx}'",
    "type-check": "tsc --noEmit",
    "jest": "TZ=Africa/Khartoum jest",
    "test": "yarn type-check && yarn jest",
    "test:watch": "yarn jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getredash/redash.git"
  },
  "engines": {
    "node": "^14.16.1",
    "yarn": "^1.22.10"
  },
  "author": "Redash Contributors",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/getredash/redash/issues"
  },
  "homepage": "https://redash.io/",
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@redash/viz": "^1.0.0",
    "ace-builds": "^1.4.12",
    "antd": "^4.4.3",
    "axios": "^0.21.1",
    "axios-auth-refresh": "^3.0.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.6",
    "d3": "^3.5.17",
    "debug": "^3.1.0",
    "dompurify": "^2.0.7",
    "font-awesome": "^4.7.0",
    "history": "^4.10.1",
    "hoist-non-react-statics": "^3.3.0",
    "markdown": "0.5.0",
    "material-design-iconic-font": "^2.2.0",
    "moment": "^2.19.3",
    "mousetrap": "^1.6.1",
    "mustache": "^2.3.0",
    "numeral": "^2.0.6",
    "path-to-regexp": "^3.1.0",
    "prop-types": "^15.6.1",
    "query-string": "^6.9.0",
    "react": "^16.14.0",
    "react-ace": "^9.1.1",
    "react-dom": "^16.14.0",
    "react-grid-layout": "^0.18.2",
    "react-resizable": "^1.10.1",
    "react-virtualized": "^9.21.2",
    "sql-formatter": "git+https://github.com/getredash/sql-formatter.git",
    "universal-router": "^8.3.0",
    "use-debounce": "^3.1.0",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.1",
    "@percy/agent": "0.24.3",
    "@percy/cypress": "^2.3.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/cypress": "^7.0.2",
    "@types/classnames": "^2.2.10",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/sql-formatter": "^2.3.0",
    "atob": "^2.1.2",
    "copy-webpack-plugin": "^4.5.3",
    "css-loader": "^0.28.7",
    "cypress": "^5.3.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "less-plugin-autoprefix": "^1.5.1",
    "mini-css-extract-plugin": "^0.4.4",
    "mockdate": "^2.0.2",
    "raw-loader": "^0.5.1",
    "react-refresh": "^0.9.0",
    "react-test-renderer": "^16.5.2",
    "request": "^2.88.0",
    "request-cookies": "^1.1.0",
    "style-loader": "^2.0.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.44.2",
    "webpack-build-notifier": "^0.1.30",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "jest": {
    "rootDir": "./src",
    "setupFiles": [
      "../jest/setupEnzyme.js",
      "../jest/mocks.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^@/(.*)": "<rootDir>/app/$1",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "nyc": {
    "include": [
      "client/app/**",
      "viz-lib/**"
    ]
  },
  "//": "browserslist set to 'Async functions' compatibility",
  "browserslist": [
    "Edge >= 15",
    "Firefox >= 60",
    "Chrome >= 55",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Opera >= 42",
    "op_mob >= 46",
    "android >= 67",
    "and_chr >= 71",
    "and_ff >= 64",
    "and_uc >= 11.8",
    "samsung >= 6.2"
  ]
}
